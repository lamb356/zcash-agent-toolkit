{
  "name": "@zcash-agent/toolkit",
  "version": "0.1.0",
  "description": "AI agent communication toolkit over Zcash shielded memo fields",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:wasm": "wasm-pack build ../crates/wasm-bindings --target web --out-dir ../../ts-sdk/wasm-pkg",
    "build:ts": "tsup",
    "build": "npm run build:wasm && npm run build:ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^3.0.0"
  },
  "keywords": [
    "zcash",
    "privacy",
    "agent",
    "ai",
    "wasm",
    "memo",
    "encryption"
  ],
  "license": "MIT OR Apache-2.0"
}
